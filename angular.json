{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@ng-dot/api": {
      "projectType": "library",
      "root": "projects/ng-dot/api",
      "sourceRoot": "projects/ng-dot/api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng-dot/api/tsconfig.lib.json",
            "project": "projects/ng-dot/api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-dot/api/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-dot/api/src/test.ts",
            "tsConfig": "projects/ng-dot/api/tsconfig.spec.json",
            "karmaConfig": "projects/ng-dot/api/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-dot/api/tsconfig.lib.json",
              "projects/ng-dot/api/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
